CREATE TABLE `users` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `login` varchar(20) CHARACTER SET latin1 NOT NULL,
  `password` varchar(20) CHARACTER SET latin1 NOT NULL,
  `lastName` varchar(100) CHARACTER SET latin1 NOT NULL,
  `firstName` varchar(100) CHARACTER SET latin1 NOT NULL,
  `mobilePhone` varchar(20) CHARACTER SET latin1 NOT NULL,
  `email` varchar(200) CHARACTER SET latin1 DEFAULT NULL,
  `sex` char(2) NOT NULL,
  `country` varchar(10) CHARACTER SET latin1 DEFAULT NULL,
  `city` varchar(100) DEFAULT NULL,
  `address` text,
  `accountType` varchar(20) DEFAULT 'particulier',
  `nbPasswordReset` int(11) DEFAULT '0',
  `lastUpdatedDateTimeOfpassword` timestamp NULL DEFAULT NULL,
  `oldPassword` varchar(20) CHARACTER SET latin1 DEFAULT NULL,
  `confirmationCode` char(10) DEFAULT NULL,
  `userLevel` int(11) DEFAULT '4',
  `isReseller` char(1) DEFAULT '0',
  `isSmsApiUser` char(1) DEFAULT '0',
  `isGoogleTranslateApiUser` char(1) DEFAULT '0',
  `defaultLang` char(40) CHARACTER SET latin1 DEFAULT '',
  `defaultSmsSubject` varchar(20) DEFAULT NULL,
  `nbInvitationSent` int(11) DEFAULT '0' COMMENT 'The number of invitation sent by the user',
  `smsBalance` int(11) DEFAULT NULL,
  `smsCredit` int(11) DEFAULT '0',
  `smsDebit` int(11) DEFAULT '0',
  `smsBonus` int(11) DEFAULT '0',
  `dailySmsBonus` int(11) DEFAULT '0',
  `nbSmsApiAccount` int(11) DEFAULT '0',
  `nbConfirmInvitations` int(11) DEFAULT '0',
  `secretQuestion` text CHARACTER SET latin1,
  `secretResponse` text CHARACTER SET latin1,
  `nbErrorAuthentication` int(11) DEFAULT '0',
  `liveStatus` char(3) DEFAULT '2',
  `createdDateTime` timestamp NULL DEFAULT NULL,
  `lastUpdatedDateTime` timestamp NULL DEFAULT NULL,
  `createdDate` date DEFAULT '0000-00-00',
  `createdHour` time DEFAULT '00:00:00',
  `creatorId` int(11) DEFAULT '0',
  `lastModifierId` int(11) DEFAULT '0',
  `universalId` varchar(100) CHARACTER SET latin1 DEFAULT '0',
  `signature` varchar(100) CHARACTER SET latin1 DEFAULT '0',
  `isaccountNonExpired` varchar(3) CHARACTER SET latin1 DEFAULT NULL,
  `isaccountNonLocked` varchar(3) CHARACTER SET latin1 DEFAULT NULL,
  `iscredentialsNonExpired` varchar(3) CHARACTER SET latin1 DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `usercompte_loginsk` (`login`),
  KEY `telephone` (`mobilePhone`),
  KEY `user_type` (`accountType`),
  KEY `user_categorie` (`userLevel`),
  KEY `nom` (`lastName`) USING BTREE
) ENGINE=MyISAM AUTO_INCREMENT=328 DEFAULT CHARSET=utf8
