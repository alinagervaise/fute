	padding: 7px;
}
.curam_textareabox	{ 
	width: 100%; 
	float: left;
}
.curam_textareabox	textarea { 
	border-radius: 0px;
	padding: 7px; 
	height: 130px; 
	width: 97.5%; 
	/*margin-top: 20px;*/
}
.curam_textareabox button { 
	border-radius: 0px; 
	background: #3cb04b; 
	padding: 7px 15px; 
	text-transform: uppercase; 
	font-weight: 400; 
	font-size: 16px;
	margin-top: 20px;
}
.curam_address ul { 
	float: left; 
	margin: 0px; 
	padding: 0px; 
}
.curam_address	li { 
	padding: 0px 0px 10px 0px; 
	margin: 0px; 
