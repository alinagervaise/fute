 display: none;}
	 #main-content { 
 width: 100%; }
 	.first-footer { 
 border-left: 0; }
	.flexslider .slides, .flexslider, .main-slider { 
 height: auto; }
	.service-item { 
 margin-bottom: 30px; }
	.progess-bars { 
